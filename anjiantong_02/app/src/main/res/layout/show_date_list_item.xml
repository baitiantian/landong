<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_examine_item">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"
            android:gravity="center"
            android:padding="5dp"
            android:ellipsize="end"
            android:background="@drawable/bg_examine_item"
            android:text="@string/personnel_management_03"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"
            android:gravity="center"
            android:id="@+id/show_date_01"
            android:padding="5dp"
            android:ellipsize="end"
            android:background="@drawable/bg_examine_item" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"
            android:gravity="center"
            android:padding="5dp"
            android:ellipsize="end"
            android:background="@drawable/bg_examine_item"
            android:text="@string/show_date_01"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"
            android:gravity="center"
            android:id="@+id/show_date_02"
            android:padding="5dp"
            android:ellipsize="end"
            android:background="@drawable/bg_examine_item" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"
            android:gravity="center"
            android:padding="5dp"
            android:ellipsize="end"
            android:background="@drawable/bg_examine_item"
            android:text="@string/show_date_02"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"
            android:gravity="center"
            android:id="@+id/show_date_03"
            android:padding="5dp"
            android:ellipsize="end"
            android:background="@drawable/bg_examine_item" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/main_color_3"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/main_color_2"/>
</LinearLayout>