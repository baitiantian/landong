<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="com.jinan.ladongjiguan.anjiantong.activity.LawDetailActivity">

    <include
        android:id="@+id/law_detail_title"
        layout="@layout/i_title_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <!--<com.jinan.ladongjiguan.anjiantong.PublicClass.MyScrollView-->
    <ScrollView
        android:id="@+id/law_detail_scrollview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/law_detail_title"
        android:layout_margin="10dp"
        android:scrollbars="none"
        android:fillViewport="true">

        <TextView
            android:id="@+id/law_detail_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/main_color_3"
            android:textSize="16sp"/>
    </ScrollView>
    <!--</com.jinan.ladongjiguan.anjiantong.PublicClass.MyScrollView>-->

</RelativeLayout>
